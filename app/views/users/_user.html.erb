
<table>
  <tr>
    <th>Name</th>
    <th>Phone Number </th>
    <th> Time </th>
    <th>Notes</th>
    <th colspan="4"></th>
  </tr>

  <tr>
    <td><%= user.name %></td>
    <td><%= user.phone_number %></td>
    <td><%= user.time %></td>
    <td><%= user.notes %></td>
    <td><%= link_to 'Edit', edit_bar_user_path(user.bar, user) %></td>
    <td><%= link_to 'Delete', [user.bar, user],
    method: :delete,
    data: { confirm: 'Are you sure?'} %></td>
  </tr>
  </table>

